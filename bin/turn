#!/usr/bin/env ruby
require_relative '../lib/turn'

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

puts "Welcome to Tic Tac Toe!"

display_board(board)

turn(board)

input = gets.strip

index = input_to_index(input)

input_to_index(input)
move(board, position, char)
valid_move?(board, index)
position_taken?(board, index)

display_board(board)
